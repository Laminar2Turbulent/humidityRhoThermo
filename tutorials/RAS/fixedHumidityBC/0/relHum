FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      relHumidity;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.3;

boundaryField
{
    walls
    {
        type            zeroGradient;
    }

    inletTop
    {
        type            fixedHumidity;
        mode            relative;
        method          magnus;
        humidity        0.50;
        value           uniform 0.0;
    }

    inletBottom
    {
        type            fixedHumidity;
        mode            relative;
        method          magnus;
        humidity        0.30;
        value           uniform 0.0;
    }

    outletTop
    {
        type            zeroGradient;
    }

    outletBottom
    {
        type            zeroGradient;
    }

    front
    {
        type            empty;
    }

    back
    {
        type            empty;
    }
}

// ************************************************************************* //
